<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <CheckBox fx:id="enableCheckBox" mnemonicParsing="false" text="Enable" />
      <Label text="NOTE: Auto battle echelons should always be in a higher echelon number than logistic echelons so they don't interfere with status reading" wrapText="true" />
      <Label text="NOTE: Disabling this will lead to restarts when the app runs into an auto battle echelon" wrapText="true" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
