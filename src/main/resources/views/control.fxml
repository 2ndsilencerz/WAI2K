<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <TableView fx:id="tableView" editable="true">
        <columns>
          <TableColumn fx:id="keyColumn" editable="false" prefWidth="75.0" text="Key" />
          <TableColumn editable="false" prefWidth="107.0" text="X" fx:id="xColumn" />
            <TableColumn fx:id="yColumn" editable="false" prefWidth="107.0" text="Y" />
            <TableColumn fx:id="varianceColumn" prefWidth="103.0" text="Variance" />
        </columns>
      </TableView>
      <HBox spacing="20.0">
         <children>
            <HBox fx:id="editHBox" spacing="10.0">
               <children>
                  <Button fx:id="setKeyButton" mnemonicParsing="false" text="Set Key" />
                  <Button fx:id="setCoordButton" mnemonicParsing="false" text="Set Coordinate" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" />
               </children>
            </HBox>
            <Button fx:id="addButton" mnemonicParsing="false" text="Add" />
         </children>
      </HBox>
   </children>
</VBox>
